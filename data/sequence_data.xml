<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Procurement Procedure Sequence -->
        <record id="seq_procurement_procedure" model="ir.sequence">
            <field name="name">Procurement Procedure</field>
            <field name="code">procurement.procedure</field>
            <field name="prefix">PROC/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Committee Session Sequence -->
        <record id="seq_committee_session" model="ir.sequence">
            <field name="name">Committee Session</field>
            <field name="code">procurement.committee.session</field>
            <field name="prefix">SESS/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

